{
  "compilerOptions": {
    "target": "ES2015",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "outDir": "../../build/main",
    "allowJs": true,
    "removeComments": true,
    "resolveJsonModule": true,
    "noImplicitAny": false,
  },
  "exclude": ["static"]
}
